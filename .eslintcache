nticatedRoute';\r\n\r\nclass TodoApp extends Component{\r\n    render(){\r\n        return(\r\n            <div className=\"TodoApp\">\r\n\r\n                {/*implementing Routing for the login and welcome components*/}\r\n                <Router>\r\n                    \r\n                    <>  \r\n                    <HeaderComponent/>\r\n                        {/*switch make sure that only one of these routes is active at any particular point in time.*/}\r\n                        <Switch>\r\n                            <Route path=\"/\" exact component={LoginComponent}/>\r\n                            <AuthenticatedRoute path=\"/welcome/:name\" component={WelcomeComponent}/>\r\n                            <Route path=\"/login\" component={LoginComponent}/>\r\n                            <AuthenticatedRoute path=\"/todos\" component={ListTodosComponent}/>\r\n                            <AuthenticatedRoute path=\"/logout\" component={LogoutComponent}/>\r\n                            <Route component={ErrorComponent}/>\r\n                        </Switch>    \r\n                    </>\r\n                    <FooterComponent/>\r\n                </Router>\r\n                \r\n              { /* <WelcomeComponent/>\r\n                <LoginComponent/>*/}\r\n                \r\n                \r\n            </div>\r\n             \r\n        );\r\n    }\r\n}\r\n\r\nfunction ErrorComponent() {\r\n    return <div>An error is Ouccured. I don't now what to do</div>    \r\n}\r\n\r\nclass ListTodosComponent extends Component{\r\n    constructor(props){\r\n        super(props)\r\n        this.state={\r\n            todos :\r\n            [\r\n            {id: 1 , description : 'Learn React', done:false, targetDate: new Date()},\r\n            {id: 2 , description : 'Go to Gym', done:false, targetDate: new Date()},\r\n            {id: 3 , description : 'Do Homework', done:false, targetDate: new Date()}\r\n            ]\r\n        }\r\n\r\n\r\n    }\r\n    render(){\r\n        return(\r\n            <div>\r\n                <h1>List Todos</h1>\r\n                <div classname=\"container\">\r\n                <table class=\"table\">\r\n                    <thead>\r\n                        <tr>\r\n                         \r\n                            <th>description</th>\r\n                            <th>done</th>\r\n                            <th>targetDate</th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        {\r\n                            this.state.todos.map(\r\n                            todo =>\r\n                            //to say that every child have unique key (id )\r\n                                <tr key={todo.id}>\r\n                                    \r\n                                    <td>{todo.description}</td>\r\n                                    <td>{todo.done.toString()}</td>\r\n                                    <td>{todo.targetDate.toString()}</td>\r\n\r\n                                </tr>\r\n                            )\r\n                         }\r\n                    </tbody>\r\n                </table>\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n}\r\nclass HeaderComponent extends Component{\r\n    render(){\r\n        //create bolean variable : true if the user is connected \r\n        const isUserLoggedIN = AuthService.isUserLoggedIn();\r\n        //console.log(isUserLoggedIN);\r\n\r\n        return(\r\n            <header>\r\n                <nav className=\"navbar navbar-expand-md navbar-dark bg-dark\">\r\n                    <div><a className=\"navbar-brand\">Todo</a></div>\r\n                    <ul className=\"navbar-nav\">\r\n                        {/*this links can be showed only if the user is connected */}\r\n                        {isUserLoggedIN && <li ><Link className=\"nav-link\" to=\"/welcome/name\">Home</Link></li>}\r\n                        {isUserLoggedIN && <li ><Link className=\"nav-link\" to=\"/todos\">Todos</Link></li>}\r\n                    </ul>\r\n                    <ul className=\"navbar-nav navbar-collapse justify-content-end \">\r\n                        <li ><Link className=\"nav-link\" to=\"/login\">LogIn</Link></li>\r\n                        {isUserLoggedIN && <li ><Link className=\"nav-link\" to=\"/logout\" onClick={AuthService.logout}>LogOut</Link></li>}\r\n                    </ul>\r\n                </nav>\r\n            </header>\r\n        )\r\n    }\r\n}\r\nclass FooterComponent extends Component{\r\n    render(){\r\n        return(\r\n            <footer className=\"footer\">\r\n                <span className=\"text-muted\">The key is in not spending time, but in investing it</span>\r\n\r\n            </footer>\r\n            \r\n        )\r\n    }\r\n}\r\nclass LogoutComponent extends Component{\r\n    render(){\r\n        return(\r\n            <div>\r\n            <h1> You are logged out </h1>\r\n            <div className=\"container\" >Thank you for Using Our Application </div>\r\n            </div>\r\n        )\r\n    }\r\n}\r\nclass WelcomeComponent extends Component{\r\n    render(){\r\n\r\n        return(\r\n           <> \r\n           <h1>Welcome</h1>\r\n           <div class=\"container\">\r\n      \r\n                Welcome {this.props.match.params.name}. You Can Manage your todos <Link to='/todos'>here </Link>\r\n            </div>\r\n            </>\r\n        )\r\n    }\r\n}\r\n\r\n// the LoginComponent a controlled component.\r\nclass LoginComponent extends Component{\r\n    constructor(props){\r\n        super(props)\r\n//We added state to this Logincomponent.\r\n        this.state ={\r\n            username: '',\r\n            password: '',\r\n            hasloFailed: false,\r\n            showSucssMess: false,\r\n\r\n        }\r\n    \r\n        this.handelChange=this.handelChange.bind(this);\r\n        this.loginClicked=this.loginClicked.bind(this);\r\n    }\r\n\r\n  